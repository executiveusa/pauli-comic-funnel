# Pauli Effect Environment Variables

# =====================================================
# COPILOTKIT CONFIGURATION
# =====================================================
# CopilotKit API Key for agent-driven UI generation
COPILOTKIT_API_KEY=your_copilotkit_api_key_here

# =====================================================
# ANTHROPIC AI
# =====================================================
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# =====================================================
# AGI OPEN CONFIGURATION
# =====================================================
# AGI Open Platform API configuration
AGI_OPEN_API_KEY=your_agi_open_api_key_here
AGI_OPEN_ENDPOINT=https://www.agiopen.org/api

# =====================================================
# NOTION INTEGRATION
# =====================================================
NOTION_API_TOKEN=your_notion_api_token_here
NOTION_DATABASE_ID=your_notion_database_id_here

# =====================================================
# SERVER CONFIGURATION
# =====================================================
PORT=3001
NODE_ENV=development

# =====================================================
# DATABASE
# =====================================================
DATABASE_URL=your_database_connection_string_here

# =====================================================
# LIBRECHAT CONFIGURATION
# =====================================================
LIBRECHAT_ENABLED=true
LIBRECHAT_AUTH_SECRET=your_librechat_auth_secret_here
LIBRECHAT_TEAM_EMAILS=team@example.com,admin@example.com
